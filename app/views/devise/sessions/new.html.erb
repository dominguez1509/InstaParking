  <section id="login">
    <div id="contact-us" class="parallax">
      <div class="container">
        <div class="row">
          <div class="heading text-center col-sm-8 col-sm-offset-2 wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
            <h2>Inicia Sesión / Regístrate</h2>
            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua ut enim ad minim veniam</p>
          </div>
        </div>
        <div class="contact-form wow fadeIn" data-wow-duration="1000ms" data-wow-delay="600ms">
          <div class="row">
            <div class="col-sm-6">
              <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
                <div class="row  wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                  <div class="row">
                    <div class="col-sm-8 col-sm-offset-2">
                      <div class="form-group">
                        <input type="email" name="user[email]" id="user_email" class="form-control" placeholder="Email Address" required="required">
                      </div>
                    </div>
                  </div>
                  <div class="row">
                    <div class="col-sm-8 col-sm-offset-2">
                      <div class="form-group">
                        <input type="password" name="user[password]" id="user_password" class="form-control" placeholder="Name" required="required">
                      </div>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <button type="submit" class="btn btn-primary btn-lg btn-block">Iniciar Sesión</button>
                </div>
              <% end %>
            </div>
            <div class="col-sm-6">
              <%= simple_form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                <div class="row  wow fadeInUp" data-wow-duration="1000ms" data-wow-delay="300ms">
                  <div class="col-sm-6">
                    <div class="form-group">
                      <input type="text" name="user[names]" id="user_names" class="form-control" placeholder="Nombres" required="required">
                    </div>
                    <div class="form-group">
                      <input type="text" name="user[last_names]" id="user_last_names" class="form-control" placeholder="Apellidos" required="required">
                    </div>
                  </div>
                  <div class="col-sm-6">
                    <div class="form-group">
                      <input type="email" name="user[email]" id="user_email" class="form-control" placeholder="Email Address" required="required">
                    </div>
                    <div class="form-group">
                      <select name="user[type_of_user_id]"  id="user_type_of_user_id" class="form-control" required="required">
                        <option value="">Seleccione que desea hacer</option>
                        <option value="1">Ofrecer Alquiler</option>
                        <option value="2">Alquilar</option>
                      </select>
                    </div>
                  </div>
                </div>
                <div class="form-group">
                  <input class="password required form-control" type="password" name="user[password]" id="user_password" placeholder="Contraseña">
                  <input class="password required form-control" type="password" name="user[password_confirmation]" id="user_password_confirmation" placeholder="Contraseña">
                </div>
                <div class="form-group">
                  <button type="submit" class="btn btn-primary btn-lg btn-block">Registrarse</button>
                </div>
              <% end %>
            </div>
          </div>
        </div>
      </div>
    </div>        
  </section>
