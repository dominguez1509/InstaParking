.row
  .col-sm-3.col-lg-3
  .col-sm-6.col-lg-6
    .panel.panel-info.panel-border.top
      .panel-heading
        %span.panel-title= 'Encuesta N° ' + @survey.id.to_s
      .panel-body
        %p.p2= 'Puntuación para el Local/Auto :' + @survey.object_score.to_s
        %p.p2= 'Puntuación para el Usuario :' + @survey.user_score.to_s
        %a.btn.btn-info{:href => "#{surveys_path}"}
          %i.fa.fa-reply
          Regresar al Listado de encuestas